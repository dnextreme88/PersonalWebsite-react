{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": { "jsx": true },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "linebreak-style": ["error", "windows"],
        "quotes": ["error", "single"],
        "semi": ["error", "never"],
        "eol-last": ["warn", "never"],
        "react/prop-types": ["off"]
    }
}
